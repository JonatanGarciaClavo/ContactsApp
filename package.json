{
  "name": "Contacts-App",
  "version": "2.1.2",
  "description": "Contacts App",
  "scripts": {
    "dev": "babel-node server.js",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "start": "NODE_ENV=production PORT=80 babel-node server.js",
    "lint": "eslint src; exit 0",
    "clean": "rimraf dist",
    "dev-win": "SET NODE_ENV=development&&babel-node server.js",
    "build-win": "rimraf dist && SET NODE_ENV=production&& webpack --config ./webpack.production.config.js --progress --profile --colors",
    "start-win": "SET NODE_ENV=production&&SET PORT=80&&babel-node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupFiles": ["./config/jest/jest-setup.js"],
    "collectCoverageFrom":[
      "**/src/**/*.js",
      "!**/__mocks__/**",
      "!**/coverage/**",
      "!**/src/**/index.js",
      "!**/src/services/**",
      "!**/src/store/**",
      "!**/__tests__/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudmu/react-redux-starter-kit.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "reduxjs",
    "react-router",
    "webpack",
    "boilerplate"
  ],
  "author": "Jonatan Garc√≠a Clavo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloudmu/react-redux-starter-kit/issues"
  },
  "homepage": "https://github.com/cloudmu/react-redux-starter-kit",
  "dependencies": {
    "bluebird": "^3.3.5",
    "extensible-runtime": "^4.0.1",
    "firebase": "^2.4.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.11.1",
    "material-ui": "^0.16.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.4",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.12.0",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-extensible-destructuring": "^4.0.1",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.4",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.8.5",
    "jest": "^16.0.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "redux-mock-store": "^1.2.1",
    "redux-saga-testing": "^0.1.1",
    "rimraf": "^2.5.2",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.1",
